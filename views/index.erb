<html>
	<head>
		<title>Graylog2 client</title>
		<link href="themes/sunny/jquery-ui-1.8.14.custom.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="themes/ui.jqgrid.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="themes/fetcherb.css" media="screen" rel="stylesheet" type="text/css" />
		<link href="themes/ui.datetimepicker.addon.css" media="screen" rel="stylesheet" type="text/css" />
		<script src="js/jquery-1.5.2.min.js" type="text/javascript"></script>
		<script src="js/jquery-ui-1.8.14.custom.min.js" type="text/javascript"></script>
		<script src="js/grid.locale-en.js" type="text/javascript"></script>
		<script src="js/jquery.jqGrid.min.js" type="text/javascript"></script>
		<script src="js/jquery.ui.datetimepicker.addon.js" type="text/javascript"></script>
		<script src="js/date.js" type="text/javascript"></script>
	</head>
	<body>
		<div style="display:none">
			<div id="dialog-confirm" title="Start logs retrieval?">
				<p>
					<span class="ui-icon ui-icon-alert" style="float:left; margin:0 7px 20px 0;"></span>
					You are about to start the fetchers. Make sure you have selected the correct hosts/files and options before
					continuing.
				</p>
			</div>
			<div id="dialog-complete" title="Request sent!">
				<p>
					<span class="ui-icon ui-icon-info" style="float:left; margin:0 7px 20px 0;"></span>
					Your request has been sent! Check progress in the console window.
				</p>
			</div>
		</div>

		<div style="margin:5px;">
			<form method="post" name="options" id="options" action="" title='' style="width:380px">
				<div class="ui-widget ui-widget-content ui-corner-all" dir="ltr">
					<div class="ui-jqgrid-titlebar ui-widget-header ui-corner-top ui-helper-clearfix">
						<span class="ui-jqgrid-title">Options</span>
					</div>
					<table>
						<tbody>
							<tr>
								<td>Start time (UTC)</td>
								<td>
									<input type="text" id="time" name="time" value="YYYY-MM-DD HH:mm:ss" role="textbox"
										class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
							<tr>
								<td>Time window</td>
								<td>
									<input type="text" name="timewindow" id="timewindow" value="60" role="textbox"
										class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
							<tr>
								<td>Private Key File</td>
								<td>
									<input type="text" name="keyfile" id="keyfile" role="textbox" class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
							<tr>
								<td>Tokens</td>
								<td>
									<input type="text" name="tokens" id="tokens" role="textbox" class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
							<tr>
								<td>Connections</td>
								<td>
									<input type="text" name="concurrent_connections" id="concurrent_connections" role="textbox"
										class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
							<tr>
								<td>Verbose</td>
								<td>
									<input type="checkbox" name="verbose" id="verbose" role="textbox" class="FormElement ui-widget-content ui-corner-all" />
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</form>
			<script src="js/fetcherb.options.js" type="text/javascript"></script>
			<p />
			<table id="grid"></table>
			<div id="pager"></div>
			<script src="js/fetcherb.jqgrid.js" type="text/javascript"></script>
			<script src="js/fetcherb.dialogs.js" type="text/javascript"></script>
		</div>
	</body>
</html>